{
    "name": "wakeonweb/behat-contexts",
    "description": "Provide some behat contexts",
    "keywords": ["behat", "contexts", "orm", "amqp"],
    "license": "MIT",
    "authors": [
        {
            "name": "Contributors",
            "homepage": "https://github.com/WakeOnWeb/behat-contexts/graphs/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "WakeOnWeb\\BehatContexts\\": "src"
        }
    },
    "require": {
        "php": "^7.1",
        "behat/behat": "~3.0"
    },
    "require-dev": {
        "behat/symfony2-extension": "^2.1",
        "internations/http-mock": "^0.12",
        "squizlabs/php_codesniffer": "~3.1.1",
        "ubirak/rest-api-behat-extension": "^7.0",
        "swaggest/json-diff": "dev-master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:atomatis/json-diff.git"
        }
    ]
}
